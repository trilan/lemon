#: admin.py:15
msgid "Publication parameters"
msgstr "Параметры публикации"

#: admin.py:34
msgid "author name"
msgstr "автор"

#: admin.py:43
#, python-format
msgid "Enable selected %(verbose_name_plural)s"
msgstr "Включить выбранные %(verbose_name_plural)s"

#: admin.py:51
#, python-format
msgid "Disable selected %(verbose_name_plural)s"
msgstr "Выключить выбранные %(verbose_name_plural)s"

#: models.py:17
msgid "author"
msgstr "автор"

#: models.py:20
msgid "enabled"
msgstr "включено"

#: models.py:21
msgid "If not set, publication is hidden from visitors anyway."
msgstr "Если выключено, публикация будет скрыта от посетителей в любом случае."

#: models.py:23
msgid "publication start date"
msgstr "дата начала показа"

#: models.py:24
msgid "Publication will be visible to visitors starting from this date."
msgstr "Публикация будет видна посетителям, начиная с этой даты."

#: models.py:27
msgid "publication end date"
msgstr "дата окончания показа"

#: models.py:28
msgid "Publication will be visible to visitors due to this date if set."
msgstr "Публикация будет видна посетителям до этой даты."
